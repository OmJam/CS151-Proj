<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="1000.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RecommendationFormController">
   <children>
      <HBox prefHeight="343.0" prefWidth="610.0">
         <children>
            <VBox alignment="TOP_RIGHT" prefHeight="384.0" prefWidth="274.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="135.0" text="First Name:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="122.0" text="Last Name: ">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="90.0" text="Gender:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="103.0" text="School: ">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="130.0" text="Todays Date:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="38.0" prefWidth="264.0" text="Program Student is Applying to: ">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="125.0" text="Semester:">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="87.0" text="Year: ">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin>
            </VBox>
            <VBox prefHeight="384.0" prefWidth="322.0">
               <children>
                  <TextField prefHeight="42.0" prefWidth="117.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField prefHeight="42.0" prefWidth="100.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
                  <ComboBox fx:id="genderSelection" prefHeight="42.0" prefWidth="150.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </ComboBox>
                  <TextField prefHeight="42.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
                  <DatePicker prefHeight="42.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </DatePicker>
                  <ComboBox fx:id="Program" prefHeight="42.0" prefWidth="150.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="Semester" prefHeight="42.0" prefWidth="150.0" promptText="Semester">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </ComboBox>
                  <TextField prefHeight="42.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
         </children>
      </HBox>
      <Label text="Grades for each Course(List in order):   " />
      <HBox prefHeight="125.0" prefWidth="610.0">
         <children>
            <VBox prefHeight="104.0" prefWidth="427.0">
               <children>
                  <ComboBox fx:id="Course1" prefHeight="38.0" prefWidth="342.0" promptText="Course 1" />
                  <ComboBox fx:id="Course2" prefHeight="38.0" prefWidth="342.0" promptText="Course 2" />
                  <ComboBox fx:id="Course3" prefHeight="38.0" prefWidth="342.0" promptText="Course 3" />
               </children>
            </VBox>
            <VBox prefHeight="100.0" prefWidth="222.0">
               <children>
                  <ComboBox fx:id="Grade1" prefWidth="150.0" promptText="Grade 1" />
                  <ComboBox fx:id="Grade2" prefWidth="150.0" promptText="Grade 2" />
                  <ComboBox fx:id="Grade3" prefWidth="150.0" promptText="Grade 3" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label text="Personal Characteristics: " />
      <ListView fx:id="listViewPC" prefHeight="88.0" prefWidth="650.0" />
      <Label text="Academic Characteristics: " />
      <ListView fx:id="listViewAC" prefHeight="83.0" prefWidth="650.0" />
      <Label prefHeight="26.0" prefWidth="320.0" text="note: hold CTRL to select multiple" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="homePageBtn" mnemonicParsing="false" onAction="#handleHomePage" text="Home Page" />
            <Button fx:id="recLogOut" alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#handleRecLogOut" style="-fx-background-color: #C70000#C70000;" text="Log Out" textFill="WHITE" />
         </children>
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
